{
  "version": 2,
  "builds": [
    {
      "src": "backend/scn_esg_platform/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/scn_esg_platform/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "scn_esg_platform.settings_render",
    "VITE_API_URL": ""
  },
  "build": {
    "env": {
      "DJANGO_SETTINGS_MODULE": "scn_esg_platform.settings_render",
      "VITE_API_URL": ""
    }
  }
}